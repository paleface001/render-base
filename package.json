{
  "name": "render-engine-base",
  "version": "0.0.0",
  "description": "visualization renderer",
  "keywords": [
    "webgl",
    "visualization"
  ],
  "main": "build/engine-base.js",
  "browser": "build/engine-base.js",
  "module": "index.js",
  "devDependencies": {
    "@lite-js/torch": "^0.2.6",
    "babel-eslint": "~7.2.3",
    "babel-loader": "~7.1.1",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.24.0",
    "chai": "^4.0.1",
    "connect": "~3.6.6",
    "electron": "~1.8.2-beta5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-egg": "^4.2.0",
    "event-simulate": "^1.0.0",
    "get-port": "~3.2.0",
    "jquery": "^3.3.1",
    "nunjucks": "~3.0.1",
    "open": "~0.0.5",
    "parseurl": "~1.3.2",
    "pre-commit": "^1.2.2",
    "serve-static": "~1.13.2",
    "shelljs": "^0.7.8",
    "uglify-js": "^3.0.15",
    "webpack": "^3.4.1",
    "webpack-cli": "^3.2.1"
  },
  "scripts": {
    "build": "webpack",
    "ci": "npm run lint && npm run test",
    "compress": "uglifyjs --compress --mangle --output dist/g2-plugin-slider.min.js -- build/g2-plugin-slider.js",
    "coverage": "npm run coverage-generator && npm run coverage-viewer",
    "coverage-viewer": "torch-coverage",
    "coverage-generator": "torch --coverage --compile --renderer --recursive test/unit",
    "demos-web": "node ./demos/app.js --web --port 2046",
    "dev": "npm run watch & npm run demos-web",
    "dist": "rm -rf dist && mkdir dist && npm run build && npm run compress",
    "prepublishOnly": "npm run dist",
    "lint": "eslint ./",
    "lint-fix": "eslint --fix ./",
    "test": "torch --compile --renderer --recursive ./test/unit",
    "test-live": "torch --compile --renderer --interactive --recursive ./test/unit",
    "watch": "webpack --config webpack-dev.config.js"
  },
  "dependencies": {},
  "pre-commit": {
    "run": [],
    "silent": false
  }
}
